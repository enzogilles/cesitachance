{% extends "layout/base.twig" %}
{% block title %}Statistiques Étudiant{% endblock %}
{% block content %}
<section class="content">
    <h2>Statistiques Étudiant</h2>
    <p>Nom : {{ etudiant.nom|e }}</p>
    <p>Prénom : {{ etudiant.prenom|e }}</p>
    <p>Email : {{ etudiant.email|e }}</p>
    <hr>
    <p>Nombre de candidatures envoyées : {{ nbCandidatures }}</p>
    <p>Nombre d'offres en wishlist : {{ nbWishlist }}</p>
    <div style="margin-top:20px;">
        <a class="btn" href="{{ url('gestionutilisateurs', 'index') }}">Retour à la liste des utilisateurs</a>
    </div>
</section>
{% endblock %}
